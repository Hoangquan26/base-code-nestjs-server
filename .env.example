NODE_ENV=development
APP_NAME=base_code_nestjs_server
APP_PORT=3000
APP_URL=http://localhost:3000


APP_CORS_ORIGIN='http://localhost:3000' # split by , ex: domain1, domain2
APP_CORS_CREDENTIALS=true
APP_CORS_METHODS=GET,HEAD,PUT,PATCH,POST,DELETE
APP_CORS_ALLOWED_HEADERS=Content-Type,Authorization,X-Request-Id,X-Trace-Id
APP_CORS_EXPOSED_HEADERS=X-Request-Id,X-Trace-Id
APP_CORS_MAX_AGE=600

LOG_LEVEL=debug
LOG_JSON=false
LOG_CONSOLE_ENABLED=true
LOG_CONSOLE_COLORIZE=true
LOG_FILE_ENABLED=false
LOG_FILE_DIR=logs
LOG_FILE_NAME=app.log
LOG_FILE_MAXSIZE=10485760
LOG_FILE_MAXFILES=5
LOG_APP_FILE_ENABLED=true
LOG_APP_FILE_NAME=app.log
LOG_ERROR_FILE_ENABLED=true
LOG_ERROR_FILE_NAME=error.log
LOG_ACCESS_FILE_ENABLED=true
LOG_ACCESS_FILE_NAME=access.log
LOG_AUDIT_FILE_ENABLED=true
LOG_AUDIT_FILE_NAME=audit.log
LOG_QUEUE_ENABLED=false
METRICS_ENABLED=true
THROTTLE_TTL_SEC=60
THROTTLE_LIMIT=100

POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=base_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/base_db?schema=public

REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

